%%
%% Simple strings:
%%
"+OK\r\n".


%%
%% Errors:
%%
"-Error message\r\n".
"-ERR unknown command 'helloworld'\r\n".
"-WRONGTYPE Operation against a key holding the wrong kind of value\r\n".


%%
%% Integers:
%%
":0\r\n".
":1000\r\n".


%%
%% Bulk strings:
%%
"$5\r\nhello\r\n".
"$7\r\nhel\r\nlo\r\n".
"$0\r\n\r\n".
"$-1\r\n".


%%
%% Arrays:
%%
"*0\r\n".
"*2\r\n$5\r\nhello\r\n$5\r\nworld\r\n".
"*3\r\n:1\r\n:2\r\n:3\r\n".

"*5\r\n"
":1\r\n"
":2\r\n"
":3\r\n"
":4\r\n"
"$5\r\n"
"hello\r\n".

"*-1\r\n".
